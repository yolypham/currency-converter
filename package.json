        "axios": "^0.18.0",
        "body-parser": "^1.18.3",
        "express": "^4.16.4",
        "nodemon": "^1.18.10",
        "react": "^16.8.1",
        "react-currency-input": "^1.3.6",
        "react-dom": "^16.8.1",
        "react-fontawesome": "^1.6.1",
        "react-scripts": "2.1.5"
    },
    "devDependencies": {
        "concurrently": "^4.0.1"
